    <!-- https://w3id.org/IIoT#disconnect_timeout -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/IIoT#disconnect_timeout">
        <rdfs:domain rdf:resource="https://w3id.org/IIoT#Wireless_sensor_network"/>
        <rdfs:domain>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/IIoT#disconnect_timeout"/>
                <owl:someValuesFrom>
                    <rdfs:Datatype>
                        <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                        <owl:withRestrictions rdf:parseType="Collection">
                            <rdf:Description>
                                <xsd:maxInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</xsd:maxInclusive>
                            </rdf:Description>
                        </owl:withRestrictions>
                    </rdfs:Datatype>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment>disconnect_timeout (Data property): in a FireFly wireless sensor network, the SAMPL messages require a disconnect timeout for replying to messages on the mobile device</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/IIoT#fireFly_nodes -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/IIoT#fireFly_nodes">
        <rdfs:domain rdf:resource="https://w3id.org/IIoT#Wireless_sensor_network"/>
        <rdfs:domain>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/IIoT#fireFly_nodes"/>
                <owl:someValuesFrom>
                    <rdfs:Datatype>
                        <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                        <owl:withRestrictions rdf:parseType="Collection">
                            <rdf:Description>
                                <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">20</xsd:minInclusive>
                            </rdf:Description>
                            <rdf:Description>
                                <xsd:maxInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">32</xsd:maxInclusive>
                            </rdf:Description>
                        </owl:withRestrictions>
                    </rdfs:Datatype>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
        <rdfs:comment>fireFly_nodes (Data property): in a FireFly wireless sensor network, each gateway is set to manage between 20 and 32 network nodes</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/IIoT#hasAccuracy -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/IIoT#hasAccuracy">
        <rdfs:domain rdf:resource="https://w3id.org/IIoT#RTD_sensor"/>
        <rdfs:domain>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/IIoT#hasAccuracy"/>
                <owl:someValuesFrom>
                    <rdfs:Datatype>
                        <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
                        <owl:withRestrictions rdf:parseType="Collection">
                            <rdf:Description>
                                <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#float">0.1</xsd:minInclusive>
                            </rdf:Description>
                            <rdf:Description>
                                <xsd:maxInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#float">1.0</xsd:maxInclusive>
                            </rdf:Description>
                        </owl:withRestrictions>
                    </rdfs:Datatype>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
